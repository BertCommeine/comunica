          "range": "xsd:integer",
          "default": 100,
          "required": true,
          "unique": true
        }
      ],
      "constructorArguments": [
        {
          "extends": "cc:ActionObserver/constructorArgumentsObject",
          "fields": [
            {
              "keyRaw": "cacheSize",
              "value": "cahc:ActionObserver/Http/Cache/cacheSize"
            }
          ]
        }
      ]
